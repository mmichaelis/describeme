<project
    xmlns="http://maven.apache.org/DECORATION/1.6.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd"
    name="DescribeMe">

  <skin>
    <!--
    See http://andriusvelykis.github.io/reflow-maven-skin/
    -->
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>

  <body>
    <links>
      <item name="About.me" href="https://about.me/michaelismark"/>
      <item name="GitHub Repository" href="http://github.com/mmichaelis/describeme/"/>
      <item name="GitHub Pages" href="http://mmichaelis.github.com/describeme/"/>
    </links>

    <breadcrumbs>
      <item href="index.html" name="${project.name}"/>
    </breadcrumbs>

    <menu ref="parent" inherit="top"/>

    <menu ref="modules" inherit="bottom"/>

    <menu ref="reports" inherit="bottom"/>

  </body>

  <custom>
    <reflowSkin>
      <theme>bootswatch-cerulean</theme>
      <!--
      Preferred but currently not available through Reflow (Reflow uses Bootswatch v2.3.2)
      <theme>bootswatch-paper</theme>
      -->
    </reflowSkin>
  </custom>
</project>
